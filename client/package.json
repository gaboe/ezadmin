{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"apollo-cache-inmemory": "^1.5.1",
		"apollo-client": "^2.5.1",
		"apollo-link": "^1.2.11",
		"apollo-link-context": "^1.0.17",
		"apollo-link-error": "^1.1.10",
		"apollo-link-http": "^1.5.14",
		"formik": "^1.5.4",
		"graphql": "^14.2.1",
		"graphql-tag": "^2.10.1",
		"lodash": "^4.17.21",
		"ramda": "^0.26.1",
		"react": "^16.8.6",
		"react-apollo": "^2.5.5",
		"react-dom": "^16.8.6",
		"react-grid-system": "^4.4.5",
		"react-router-dom": "^5.0.0",
		"react-scripts": "^3.0.1",
		"react-toastify": "^5.1.0",
		"semantic-ui-react": "^0.87.1",
		"styled-components": "^4.2.0",
		"yup": "^0.27.0"
	},
	"devDependencies": {
		"@types/graphql": "^14.2.0",
		"@types/jest": "24.0.13",
		"@types/lodash": "^4.14.129",
		"@types/node": "^12.0.2",
		"@types/ramda": "^0.26.8",
		"@types/react": "^16.8.17",
		"@types/react-dom": "^16.8.4",
		"@types/react-router-dom": "^4.3.3",
		"@types/styled-components": "^4.1.15",
		"@types/yup": "^0.26.13",
		"apollo": "^2.11.1",
		"typescript": "^3.4.5"
	},
	"scripts": {
		"dev": "yarn gql && yarn start",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"gql": "yarn get-schema && yarn generate-types",
		"get-schema": "apollo schema:download --endpoint http://localhost:12355/graphql/ \"./src/domain/generated/schema.json\"",
		"generate-types": "apollo client:codegen --includes=\"./src/graphql/**/*.ts\" --localSchemaFile=\"./src/domain/generated/schema.json\" --target=typescript --addTypename --outputFlat \"./src/domain/generated/types.ts\" "
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
